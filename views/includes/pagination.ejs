<section class="pagination">
    <% if (hasPreviousPage) { %>
        <a href="?page=<%= currentPage - 1 %>">Prev</a>
        <% } %>
            <% for (let i=1; i <=lastPage; i++) { %>
                <a class="<%= currentPage == i ? 'active' : '' %>" href="?page=<%= i %>">
                    <%= i %>
                </a>
                <% } %>
                    <% if (hasNextPage) { %>
                        <a href="?page=<%= currentPage + 1 %>">Next</a>
                        <% } %>
</section>